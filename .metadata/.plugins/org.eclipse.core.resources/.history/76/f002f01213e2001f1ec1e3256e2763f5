public class Exercice1a implements It1 {
	public int apply(int t) {
		return t+1;
	}

public class Exercice1b implements It2<Exercice1> {
	public int apply(Exercice1 t1) {
		return t1.apply();
	}	
	
public static void main(String[] args) {
	Exercice1 ex1a=new Exercice1();
	System.out.println(ex1a.apply(3));
}
}